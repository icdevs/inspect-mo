service : {
  // User management methods
  createUser: (name: text, email: text) -> (nat);
  getUser: (userId: nat) query -> (opt record { name: text; email: text; id: nat });
  updateUser: (userId: nat, name: opt text, email: opt text) -> (bool);
  deleteUser: (userId: nat) -> (bool);
  
  // Simple methods for testing
  health: () query -> (text);
  increment: (value: nat) -> (nat);
  
  // Complex types
  processData: (data: vec nat8, options: record { compress: bool; encrypt: bool }) -> (result: variant { ok: text; err: text });
}
